{
	"name": "nemo",
	"displayName": "Nemo",
	"description": "Pull, edit, test, and push Aquarium protocols.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.connect"
	],
	"main": "./extension.js",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "Nemo",
			"properties": {
				"nemo.localAquariumServerUrl": {
					"type": "string",
					"default": "http://localhost:3000"
				},
				"nemo.localAquariumServerUser": {
					"type": "string",
					"default": "neptune"
				},
				"nemo.localAquariumServerPassword": {
					"type": "string",
					"default": "aquarium"
				}
			}
		},
		"views": {
			"explorer": [
				{
					"id": "aquariumOperationTypes",
					"name": "Aquarium"
				}
			]
		},
		"commands": [
			{
				"command": "extension.connect",
				"title": "Connect",
				"category": "Nemo"
			}
		]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^8.10.25",
		"eslint": "^4.19.1",
		"extend": "^3.0.2",
		"request": "^2.88.0",
		"typescript": "^3.1.4",
		"underscore": "^1.9.1",
		"vscode": "^1.1.25"
	},
	"dependencies": {
		"fs-extra": "^7.0.1"
	}
}
